{
  "name": "training_app",
  "version": "0.1.0",
  "description": "Select an excerise and record duration and intensity",
  "main": "index.js",
  "author": "Mao <maot.tm@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/react-router-dom": "^4.2.4",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "lodash": "^4.17.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "styled-components": "^3.0.2",
    "typescript": "^2.7.2"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "test": "jest --coverage",
    "test:report": "open coverage/lcov-report/index.html",
    "test:watch": "jest --watch",
    "format": "prettier --write --single-quote --print-width=120 --parser=flow --tab-width=2 \"src/**/*.{ts, tsx}\"",
    "lint": "eslint **/*.{ts, tsx} --quiet"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.ts",
      "*.tsx",
      "*.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 50,
        "branches": 50,
        "functions": 50,
        "lines": 100
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/express-jwt": "^0.0.38",
    "@types/jest": "^22.1.3",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.15",
    "@types/redux": "^3.6.0",
    "@types/webpack-env": "^1.13.5",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.2",
    "jsdom": "^11.6.1",
    "module-resolver": "^1.0.0",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "source-map-loader": "^0.2.3",
    "start-server-webpack-plugin": "^2.2.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-node-externals": "^1.6.0"
  }
}
